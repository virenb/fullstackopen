{
    "env": {
        "browser": true,
        "es6": true,
        "jest/globals": true,
        "cypress/globals": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "jest",
        "cypress"
    ],
    "rules": {
        "react/jsx-filename-extension": 0,
        "linebreak-style": 0,
        "react/prop-types": 0,
        "no-alert": 0,
        "no-shadow": 0,
        "jsx-a11y/label-has-associated-control": 0
    }
}